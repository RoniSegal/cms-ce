<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

    <context:component-scan base-package="com.enonic.cms.core.calendar"/>
    <context:component-scan base-package="com.enonic.cms.core.content"/>
    <context:component-scan base-package="com.enonic.cms.core.security"/>
    <context:component-scan base-package="com.enonic.cms.core.resource"/>
    <context:component-scan base-package="com.enonic.cms.core.structure"/>
    <context:component-scan base-package="com.enonic.cms.core.preference"/>

    <bean id="categoryService" class="com.enonic.cms.core.content.category.CategoryServiceImpl">
        <property name="timeService" ref="timeService"/>
        <property name="keyService" ref="keyService"/>
    </bean>

</beans>
